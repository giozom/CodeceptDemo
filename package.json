{
  "name": "CodeceptDemo",
  "version": "0.0.0",
  "description": "Demo using PHP travel to demo codecept testing",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "codeceptjs": "0.4.12",
    "konfig": "0.2.1",
    "mocha-junit-reporter": "1.12.0",
    "pg": "6.1.0",
    "protractor": "4.0.10"
  },
  "scripts": {
    "test": "cross-env SUITE=acceptance codeceptjs run --steps",
    "test:wip": "cross-env SUITE=acceptance codeceptjs run --steps --grep '@wip'",
    "test:e2e": "cross-env SUITE=e2e codeceptjs run --steps",
    "test:e2e:wip": "cross-env SUITE=e2e codeceptjs run --steps --grep '@wip'",
    "test:e2e:ci": "cross-env NODE_ENV=ci SUITE=e2e codeceptjs run --reporter mocha-junit-reporter",
    "test:ci": "cross-env NODE_ENV=ci SUITE=acceptance codeceptjs run --reporter mocha-junit-reporter",
    "test:ci:steps": "cross-env NODE_ENV=ci SUITE=acceptance codeceptjs run --steps",
    "webdriver:update": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giozom/CodeceptDemo.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "assert": "1.4.1",
    "cross-env": "2.0.1",
    "js-yaml": "^3.6.1",
    "webdriverio": "^4.4.0"
  }
}
